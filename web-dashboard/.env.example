# Environment variables for Cognify Dashboard
# Copy this file to .env and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=cognify-ai.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=cognify-ai
VITE_FIREBASE_STORAGE_BUCKET=cognify-ai.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Gemini API (optional - primarily used in extension)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
